{
    "name": "openclaw-plugin-wecom",
    "version": "1.5.1",
    "description": "Enterprise WeChat AI Bot channel plugin for OpenClaw",
    "type": "module",
    "main": "index.js",
    "files": [
        "index.js",
        "card-builder.js",
        "client.js",
        "crypto.js",
        "dynamic-agent.js",
        "heartbeat-manager.js",
        "image-processor.js",
        "logger.js",
        "media-handler.js",
        "message-queue.js",
        "push-service.js",
        "README.md",
        "README_ZH.md",
        "LICENSE",
        "CONTRIBUTING.md",
        "stream-manager.js",
        "utils.js",
        "wecom-app-client.js",
        "webhook.js",
        "openclaw.plugin.json"
    ],
    "dependencies": {
        "fast-xml-parser": "^4.2.0"
    },
    "peerDependencies": {
        "openclaw": "*"
    },
    "openclaw": {
        "extensions": [
            "./index.js"
        ],
        "channel": {
            "id": "wecom",
            "label": "Enterprise WeChat",
            "selectionLabel": "Enterprise WeChat (AI Bot)",
            "docsPath": "/channels/wecom",
            "docsLabel": "wecom",
            "blurb": "Support for Enterprise WeChat (WeCom) AI Bot integration",
            "order": 90,
            "aliases": [
                "wecom",
                "wework"
            ]
        },
        "install": {
            "defaultChoice": "npm",
            "npmSpec": "openclaw-plugin-wecom"
        }
    },
    "keywords": [
        "openclaw",
        "wecom",
        "enterprise-wechat",
        "chat",
        "plugin"
    ],
    "author": "",
    "license": "ISC"
}
