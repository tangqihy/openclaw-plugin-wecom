{
    "name": "openclaw-plugin-wecom",
    "version": "0.2.2",
    "description": "Enterprise WeChat AI Bot channel plugin for OpenClaw",
    "type": "module",
    "main": "index.js",
    "files": [
        "index.js",
        "client.js",
        "config.js",
        "crypto.js",
        "dynamic-agent.js",
        "logger.js",
        "README.md",
        "README_EN.md",
        "LICENSE",
        "CONTRIBUTING.md",
        "stream-manager.js",
        "types.js",
        "utils.js",
        "webhook.js"
    ],
    "dependencies": {
        "fast-xml-parser": "^4.2.0"
    },
    "openclaw": {
        "extensions": [
            "./index.js"
        ],
        "channel": {
            "id": "wxwork",
            "label": "Enterprise WeChat",
            "selectionLabel": "Enterprise WeChat (AI Bot)",
            "docsPath": "https://github.com/sunnoy/openclaw-plugin-wecom",
            "blurb": "Support for Enterprise WeChat (WeCom) AI Bot integration",
            "aliases": [
                "wecom",
                "wework"
            ]
        },
        "install": {
            "defaultChoice": "npm",
            "npmSpec": "openclaw-plugin-wecom"
        }
    },
    "keywords": [
        "openclaw",
        "wxwork",
        "wecom",
        "chat",
        "plugin"
    ],
    "author": "",
    "license": "ISC"
}
